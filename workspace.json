    "shared-frontend-hooks": {
      "root": "libs/shared/frontend/hooks",
      "sourceRoot": "libs/shared/frontend/hooks/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
